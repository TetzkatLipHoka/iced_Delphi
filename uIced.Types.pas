type
  NativeUInt = Cardinal;
//  NativeInt  = Integer;
{$IFEND}

type
  TIcedBitness = (
    bt16 = 16,
    bt32 = 32,
    bt64 = 64
  );

  TIcedFormatterType = (
    ftMasm,
    ftNasm,
    ftGas,
